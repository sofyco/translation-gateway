{
    "name": "sofyco/translation-gateway",
    "type": "library",
    "description": "Translation Gateway",
    "keywords": ["php", "translation", "gateway", "library"],
    "homepage": "https://github.com/sofyco/translation-gateway",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofiia Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "symfony/http-client": "^7.0",
        "symfony/http-foundation": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Sofyco\\Translation\\Gateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofyco\\Translation\\Gateway\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
